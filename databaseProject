SELECT Fname FNAME, Lname LNAME, memberId ID 
FROM members
WHERE member Id>300;


SELECT membershipID shipID, membershipType memtype, fee FEES 
FROM memberships 
WHERE fee = (SELECT MIN (fee FROM memberships) ;


SELECT membershipID memID, membershipType memType ,startDate, endDate,MAX fee) 
FROM memberships 
WHERE membershipID>1200 
GROUP BY membershipID 
HAVING MAX fee) > (SELECT MAX (fee) FROM memberships WHERE membershipID=1201) 
ORDER BY startDate;


SELECT equipID EQid, equipName eqNAME , purchaseDate, memberID ,trainerID 
FROM equipment 
WHERE equipID<404 
GROUP BY memberID;



SELECT membershipType, startDate, endDate, MAX(fee) 
FROM memberships 
WHERE membershipType="monthly" 
GROUP BY membershipId 
HAVING MAX (fee) ›(SELECT MAX(fee) FROM memberships WHERE membershipID="1201") 
ORDER BY membershipId DESC;



SELECT I. memberID, I. Fname, I.Lname, I. trainerID, D.phoneNO 
FROM members I, trainers D 
WHERE I. trainerId=D. trainerID;



